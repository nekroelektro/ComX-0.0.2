    "redux-thunk": "^2.3.0",
    "sass": "^1.10.0",
    "sass-loader": "^7.0.3",
    "styled-components": "^3.3.3",
    "yup": "^0.25.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --watchAll",
