@using React.AspNet
@model ComX_0._0._2.Views.Articles.Models.DtoModels.CategoryDto
@{
    var isAjax = Request.IsAjaxRequest();
    Layout = isAjax ? null : "~/Views/Articles/_Articles.cshtml";
    var subId = ViewBag.SubId;
    ViewBag.Title = string.Format("{0} - NekroPlaza - doświadcz dobroci", Model.CategoryName);
}
@Html.React("Categories", new {model = Model, isIndex = isAjax, sub = subId})

